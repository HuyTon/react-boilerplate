{
  "name": "react-boierplate",
  "version": "1.0.0",
  "description": "A boilerplate to build web application using React, Redux and Webpack. Unit testing with Jest framework and Enzyme",
  "main": "src/server/index.js",
  "scripts": {
    "build": "webpack -p --progress --config=webpack/webpack.prod.js --mode production",
    "start": "env-cmd -f .env.production npm run build",
    "dev": "webpack-dev-server --mode development --config ./webpack/webpack.dev.js --hot",
    "test": "jest --config jest.config.json",
    "test:watch": "jest --watch"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "__tests__/setup/setupEnzyme.js",
    "testPathIgnorePatterns": [
      "__tests__/setup/"
    ]
  },
  "author": "Huy Ton",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.19.2",
    "babel-polyfill": "^6.26.0",
    "chunk-manifest-webpack-plugin": "^1.1.2",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "dotenv-webpack": "^1.7.0",
    "env-cmd": "^10.0.1",
    "es6-promise": "^4.2.8",
    "express": "^4.16.3",
    "namor": "^2.0.2",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-loader-spinner": "^3.1.14",
    "react-redux": "^7.2.0",
    "react-router": "^3.2.6",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "styled-components": "^5.0.1",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack-chunk-hash": "^0.6.0",
    "webpack-merge": "^4.2.2"
  },
  "devDependencies": {
    "@appbaseio/reactivesearch": "^3.12.7",
    "@auth0/auth0-react": "^1.1.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "auth0-js": "^9.14.0",
    "babel-eslint": "^10.0.0",
    "babel-loader": "^8.0.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "body-parser": "^1.19.0",
    "clean-webpack-plugin": "^1.0.0",
    "concurrently": "^4.0.0",
    "css-loader": "^2.0.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.4",
    "eslint": "^5.0.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "file-loader": "^3.0.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^26.0.1",
    "loglevel": "^1.7.0",
    "loglevel-plugin-remote": "^0.6.8",
    "lru-cache": "^4.1.5",
    "morgan-body": "^2.5.2",
    "nodemon": "^1.17.3",
    "react-code-splitting": "^1.2.1",
    "react-test-renderer": "^16.13.1",
    "redis": "^3.0.2",
    "redux-mock-store": "^1.5.4",
    "style-loader": "^0.23.0",
    "url-loader": "^1.0.1",
    "webpack": "^4.5.0",
